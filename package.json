{
  "name": "typescript-uml",
  "displayName": "Typescript UML",
  "description": "Create uml diagrams of typescript files",
  "version": "0.0.1",
  "publisher": "ottostreifel",
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tsuml.show"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "tsuml.show",
        "title": "Show typescript uml"
      }
    ],
    "keybindings": [
      {
        "command": "tsuml.show",
        "key": "Shift+Alt+T"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/jquery": "^3.2.8",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "cytoscape": "^3.1.3",
    "jquery": "^3.2.1",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
